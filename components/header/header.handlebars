<div id="header" class="header">
    <img alt="{{alt}}" class="header__img" src="{{dzenLogoSrc}}">
    <div class="header-controls">
        <div class="header-controls-create-button">
            <img alt="{{alt}}" src="{{iconCreateSrc}}" class="header-controls-create-button__img">
            <a href="{{CREATE_POST}}" class="header-controls-create-button__content">
                {{text}}
            </a>
        </div>
        <div class="header-controls-video-button">
            <img src="{{iconVideoSrc}}" alt="{{alt}}" class="header-controls-video-button__img">
            <a href="{{VIDEO}}" class="header-controls-video-button__content">
                {{text}}
            </a>
        </div>
        <div class="header-controls-search-button">
            <img src="{{iconVideoSrc}}" alt="{{alt}}" class="header-controls-search-button__img">
            <a href="{{SEARCH}}" class="header-controls-search-button__content">
                "{{text}}"
            </a>
        </div>

    </div>
</div>
